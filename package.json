{
  "name": "javafx-controller-support",
  "displayName": "JavaFX Controller Support",
  "publisher": "your-publisher-name",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "javafx-controller-support.addAllMissingFxIds",
        "title": "JavaFX Controller: Add All Missing @FXML Fields"
      },
      {
        "command": "javafx-controller-support.addInitializeMethod",
        "title": "JavaFX Controller: Add initialize()"
      }
    ],
    "languages": [
      {
        "id": "java",
        "extensions": [
          ".java"
        ],
        "aliases": [
          "Java",
          "java"
        ]
      }
    ],
    "codeActions": [
      {
        "language": "java",
        "kind": "quickfix",
        "title": "Add Missing @FXML Fields"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}